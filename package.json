{
  "name": "galenframework-protractor-plugin",
  "version": "0.0.2-dev",
  "description": "An protractor plugin to connect Galenframework with Protractor for page interaction",
  "homepage": "https://github.com/hypery2k/galenframework-protractor-plugin",
  "bugs": {
    "url": "https://github.com/hypery2k/galenframework-protractor-plugin/issues"
  },
  "keywords": [
    "galen",
    "galenframework",
    "responsive",
    "test",
    "testing",
    "webdriver",
    "protractor",
    "protractor-plugin",
    "selenium",
    "TDD",
    "BDD"
  ],
  "author": "Martin Reinhardt",
  "scripts": {
    "install": "node install.js",
    "postinstall": "node postinstall.js",
    "test": "nodeunit --reporter=minimal test/tests.js",
    "pretest": "jshint lib",
    "coverage": "istanbul -- cover nodeunit test/tests.js && istanbul-coveralls --no-rm"
  },
  "bin": {
    "galen": "bin/galen"
  },
  "preferGlobal": "true",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "fs-extra": "^0.23.0",
    "kew": "0.6.0",
    "npmconf": "2.1.2",
    "progress": "1.1.8",
    "request": "2.87.0",
    "request-progress": "0.3.1",
    "which": "^1.1.1",
    "yargs": "^3.18.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.3",
    "istanbul": "^0.3.17",
    "istanbul-coveralls": "^1.0.3",
    "jscoverage": "^0.6.0",
    "jshint": "^2.8.0",
    "nodeunit": "^0.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hypery2k/galenframework-protractor-plugin.git"
  },
  "license": "MIT",
  "_galenVersion": "2.0.5",
  "_npmVersion": "2.6.0",
  "_nodeVersion": "1.4.1"
}

